{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions":{
		"ecmaVersion": "6",
		"sourceType": "module"
	},
	"extends": "airbnb",
	"rules": {
		"semi": ["error","always"],
		"react/jsx-filename-extension": [0],
		"keyword-spacing": [0],
		"space-before-blocks": [0],
		"indent": ["error",4],
		"no-tabs": [0],
		"react/jsx-indent": [0]
	}
}
